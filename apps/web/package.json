{
  "name": "@lvmann/web",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "deploy": "pnpm run build && wrangler deploy --minify",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "cf-typegen": "wrangler types && mv worker-configuration.d.ts src/worker-configuration.d.ts",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "wrangler types --env-interface WorkerEnv src/worker-env.d.ts"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250303.0",
    "@eslint/compat": "^1.2.7",
    "@sveltejs/adapter-cloudflare-workers": "^2.8.0",
    "@sveltejs/kit": "^2.17.3",
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "@lvmann/eslint-config": "workspace:*",
    "eslint-plugin-svelte": "^3.0.2",
    "svelte": "^5.22.0",
    "svelte-check": "^4.1.4",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.26.0",
    "vite": "^6.2.0",
    "wrangler": "^3.112.0"
  }
}
